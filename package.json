{
	"name": "fa-lang",
	"version": "0.1.0",
	"description": "Fa is a programming language which is intended to be as fast as low-level languages like C++ and Rust and as easy to use as high-level languages like Python and Javascript.",
	"main": "index.js",
	"type": "module",
	"scripts": {
		"build": "",
		"run": "",
		"dev": "",
		"calculator": "nim r --backend:js --verbosity:0 sandbox/calculator/main.nim",
		"calculator:dev": "bun run calculator && chokidar 'sandbox/calculator/**/*.nim' -c 'bun run calculator'",
		"test": "echo 'No test specified'",
		"old_build": "node scripts/build.js",
		"old_generate": "node scripts/utils/generate.js src/fa.hpp lib/fa.hpp",
		"old_laboratory": "node laboratory",
		"old_laboratory/client/:dev": "npm run dev --prefix laboratory/client",
		"old_laboratory/client/:build": "npm run build --prefix laboratory/client"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Lepzulnag/Fa.git"
	},
	"author": "lepzulnag",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Lepzulnag/Fa/issues"
	},
	"homepage": "https://github.com/Lepzulnag/Fa#readme",
	"devDependencies": {
		"chokidar-cli": "^3.0.0"
	}
}