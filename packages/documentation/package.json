		"@types/js-yaml": "^4.0.9",
		"@types/node": "^20.11.20",
		"chokidar-cli": "^3.0.0",
		"js-yaml": "^4.1.0",
		"shiki": "^3.21.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.3.5",
		"typescript": "^5.3.3",
