{
	"name": "fa",
	"scopeName": "source.fa",
	"repository": {},
	"patterns": [
		{
			"name": "comment.block.fa",
			"begin": "---",
			"end": "---"
		},
		{
			"name": "comment.line.double-dash.fa",
			"match": "--.*$"
		},
		{
			"name": "keyword.control.fa",
			"match": "\\b(type|enum|union|function|let|mutable|static|fields|errors|reactive|derived|namespace|for|when|if|else|loop|break|continue|return|do|while|export|use|async|await|yield|no|not|and|or|is|in|extract|insert)\\b"
		},
		{
			"name": "keyword.control.fa",
			"match": "@for\\b"
		},
		{
			"name": "keyword.operator.fa",
			"match": "(<<|>>|\\|\\|>|\\|>|\\||&|\\+(?!\\=)|\\-(?!\\=)|/(?!\\=)|\\*(?!\\=))"
		},
		{
			"name": "constant.language.fa",
			"match": "\\b(true|false|null)\\b"
		},
		{
			"name": "string.quoted.double.fa",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.fa",
					"match": "\\\\."
				},
				{
					"name": "meta.interpolation.fa",
					"begin": "\\{(?!\\{)",
					"beginCaptures": {
						"0": {
							"name": "punctuation.definition.interpolation.begin.fa"
						}
					},
					"end": "\\}",
					"endCaptures": {
						"0": {
							"name": "punctuation.definition.interpolation.end.fa"
						}
					},
					"contentName": "source.fa",
					"patterns": [
						{
							"include": "$self"
						}
					]
				}
			]
		},
		{
			"name": "constant.numeric.fa",
			"match": "\\b\\d+(?:\\.\\d+)?\\b"
		},
		{
			"name": "support.type.fa",
			"match": "\\b[A-Z][A-Za-z0-9_]*\\b"
		}
	]
}
